                    margin-right: $ui-item-indent-no-separator;
                }

                &.#{$prefix}menu-item-indent-right-icon {
                    margin-left: $ui-item-indent-right-icon;
                }

                &.#{$prefix}menu-item-indent-right-arrow {
                    margin-left: $ui-item-indent-right-arrow;
                }
            }
        }
    }

