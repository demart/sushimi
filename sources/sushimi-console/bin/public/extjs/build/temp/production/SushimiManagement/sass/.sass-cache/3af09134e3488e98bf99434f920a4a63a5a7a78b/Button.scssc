    @if $include-slicer-gradient {
        @if $background-gradient-focus != null {
            .#{$prefix}nlg {
                .#{$prefix}btn-focus.#{$prefix}btn-#{$ui} {
                    background-image: slicer-background-image('btn-focus.#{$prefix}btn-#{$ui}', 'btn/btn-#{$ui}-focus-bg');
                }
            }
        }

        @if $background-gradient-over != null {
            .#{$prefix}nlg {
                .#{$prefix}btn-over.#{$prefix}btn-#{$ui} {
                    background-image: slicer-background-image('btn-over.#{$prefix}btn-#{$ui}', 'btn/btn-#{$ui}-over-bg');
                }
            }
        }

        @if $background-gradient-pressed != null {
            .#{$prefix}nlg {
                .#{$prefix}btn-menu-active.#{$prefix}btn-#{$ui},
                .#{$prefix}btn-pressed.#{$prefix}btn-#{$ui} {
                    background-image: slicer-background-image('btn-pressed.#{$prefix}btn-#{$ui}', 'btn/btn-#{$ui}-pressed-bg');
                }
            }
        }

        @if $background-gradient-disabled != null {
            .#{$prefix}nlg {
                .#{$prefix}btn-disabled.#{$prefix}btn-#{$ui} {
                    background-image: slicer-background-image('btn-disabled.#{$prefix}btn-#{$ui}', 'btn/btn-#{$ui}-disabled-bg');
                }
