        bottom($ui-header-padding)
        left($ui-header-padding) + left($ui-header-border-width);

    .#{$prefix}panel-#{$ui} {
        @if not is-null($ui-border-color) { border-color: $ui-border-color; }
        padding: $ui-padding;
